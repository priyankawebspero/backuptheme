<script>
 window.addEventListener('message', (event) => {
  if (event.data && event.data.type && event.data.type.startsWith('fw:')) {
    if(event.data.type == "fw:player:quit" ||
       event.data.type == "fw:player:transition"){  
        cartUtil.updateDropdownCart();                      

  }  
    else if(event.data.type == "fw:shopping:cart-updated"){
        setTimeout(function() {
         cartUtil.updateDropdownCart();                      
       }, 300);
    }
  }      
})
 
