<script>
  $(function () {
    var stickyHeaderTop = $('#stickytypeheader').offset().top;
    var top = '10vw'
    if (screen.width > 480) {
      top = '6vw'
    }
    else {
      top = '11vw'
    }

    $(window).scroll(function () {
      if ($(window).scrollTop() > stickyHeaderTop) {
        $('#stickytypeheader').css({ position: 'fixed', top: top, width: '100%', background: 'rgb(252,247,254)' });
        $('#sticky').css('display', 'block');
      } else {
        $('#stickytypeheader').css({ position: 'static', top: top, width: '100%', background: 'rgb(252,247,254)' });
        $('#sticky').css('display', 'none');
      }
    });
  });
</script>
