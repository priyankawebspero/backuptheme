<style>
  .feature-header-icon ol {
    display: flex;
    justify-content: center;
  }
  .feature-header-icon li {
    margin: 0 10px;
    border:1px solid transparent;
    border-radius:50%;
    cursor:pointer;
    position: relative;
    margin-bottom: 17px !important;
  }
  .feature-header-icon img.arrow-up {
    position: absolute;
    bottom: -17px;
    left: 0;
    right: 0;
    margin: 0 auto;
    opacity:0;
  }
  .feature-header-icon li.active {
    border:1px solid #5e0c8c;
  }
  .feature-header-icon li.active img.arrow-up {
    opacity:1;
  }
  .page-ingredients .small-banner {
    align-items: center;
  }
  .page-ingredients .small-banner h2 {
    margin-bottom: 0;
    color: #5e0c8c;
    font-weight: 400;
    padding: 25px 0;
    font-family: roboto;
  }
  .feature-wrapper{
    margin: 30px auto 40px;
  }
  .slider-container{
    position:relative;
    margin: 80px auto 0;
  }
  .slider-wrapper {
    position: absolute;
    left: 0;
    right: 0;
    opacity: 0;
    top:0;
    max-width:100%;
    width: 1024px;
    margin: 0 auto;
  }
  .slider-wrapper.slider-tab1{
    opacity:1;
    z-index:70;
  }
  .slider-tab0{
    opacity:0;
  }
  .feature-wrapper .slick-arrow{
    background-color: transparent;
    border: 1px solid transparent;
  }
  .feature-wrapper .slick-arrow svg{
    fill: #5e0c8c;
    stroke: #5e0c8c;
  }
  .feature-wrapper .slick-dots {
    position: absolute;
    left: 0;
    bottom: -30px;
    right: 0;
  }
  .feature-wrapper  .slick-dots li button {
    background: gray;
  }
  .feature-wrapper  .slick-dots li.slick-active button {
    background: #7806ac;
  }
  .feature-wrapper .slick-initialized .slick-slide {
    display: block;
    padding: 0 20px;
  }
  .feature-wrapper .slick-arrow.slick-prev {
    left: -20px;
  }
  .feature-wrapper .slick-arrow.slick-next {
    right: -20px;
  }
  @media (max-width:767px){
    .page-ingredients .small-banner h2 {
      padding-bottom: 0;
    }
    .slider-container{
      position:relative;
      margin: 40px auto 0;
      padding: 0 0;
    }
    .feature-wrapper {
      margin: 30px auto 20px;
    }
    .feature-header-icon ol {
      justify-content: start;
      overflow: auto;
    }
    .feature-header-icon li {
      margin: 0 8px;
      min-width: 90px;
    }
  }
  @media (min-width:1200px){
    .slider-container{
      padding: 0 80px;
      margin: 20px auto 0;
    }
  }
</style>
