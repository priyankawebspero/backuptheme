<script>
  $(document).ready(function(){
    $(".feature-header-icon li img").on("click",function(){
      var sliderCount = $(this).attr("data-slider-tab");
      $(".feature-header-icon li").removeClass("active");
      $(this).closest("li").addClass("active");
      $(".slider-wrapper").css({"opacity":"0","z-index":"unset"});
      $("."+sliderCount).css({"opacity":"1","z-index":"70"});
    });
  });
</script>

